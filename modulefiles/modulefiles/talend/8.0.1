#%Module1.0
##
## Talend Open Studio Data Integration modulefile
##
proc ModulesHelp { } {
    puts stderr "This module loads Talend Open Studio for Data Integration"
    puts stderr ""
    puts stderr "Version: 8.0.1"
    puts stderr ""
    puts stderr "Usage: module load talend/8.0.1"
}

module-whatis "Talend Open Studio for Data Integration 8.0.1"

# for Tcl script use only
set	version		8.0.1
set	prefix		/opt/talend/$version
#set javahome    /usr/lib/jvm/java-11-openjdk-amd64

prepend-path  PATH              $prefix
#prepend-path  PATH              $javahome/bin
prepend-path  LD_LIBRARY_PATH   $prefix/configuration

setenv TALEND_HOME $prefix
setenv TALEND_VERSION $version
#setenv JAVA_HOME $javahome
